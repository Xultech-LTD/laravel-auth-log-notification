{
    "name": "xultech/auth-log-notification",
    "description": "Log user authentication events and send login alerts via Mailable and Notification.",
    "type": "library",
    "require": {
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "php": "^8.1",
        "jenssegers/agent": "^2.6",
        "torann/geoip": "^1.0",
        "laravel/slack-notification-channel": "^3.5",
        "laravel/vonage-notification-channel": "^3.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Xultech\\AuthLogNotification\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xultech\\AuthLogNotification\\AuthLogNotificationServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Michael Erastus",
            "email": "michael.erastus@xultechng.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "illuminate/database": "^12.1",
        "illuminate/notifications": "^12.1",
        "illuminate/mail": "^12.1",
        "illuminate/http": "^12.1",
        "pestphp/pest": "^3.7",
        "illuminate/auth": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/events": "^12.2",
        "mockery/mockery": "^1.6",
        "illuminate/cookie": "^12.2",
        "illuminate/hashing": "^12.2",
        "illuminate/config": "^12.1"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
